{
    "name": "api-platform/doctrine-odm",
    "description": "Doctrine MongoDB ODM bridge",
    "type": "library",
    "keywords": [
        "Doctrine",
        "ODM",
        "MongoDB",
        "API",
        "REST",
        "GraphQL"
    ],
    "homepage": "https://api-platform.com",
    "license": "MIT",
    "authors": [
        {
            "name": "KÃ©vin Dunglas",
            "email": "kevin@dunglas.fr",
            "homepage": "https://dunglas.fr"
        },
        {
            "name": "API Platform Community",
            "homepage": "https://api-platform.com/community/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "api-platform/doctrine-common": "^4.2.9",
        "api-platform/metadata": "^4.2",
        "api-platform/state": "^4.2.4",
        "doctrine/mongodb-odm": "^2.10",
        "symfony/property-info": "^6.4 || ^7.1 || ^8.0",
        "symfony/type-info": "^7.3 || ^8.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.11 || ^3.1",
        "doctrine/mongodb-odm-bundle": "^5.0",
        "phpspec/prophecy-phpunit": "^2.2",
        "phpunit/phpunit": "11.5.x-dev",
        "symfony/cache": "^6.4 || ^7.0 || ^8.0",
        "symfony/framework-bundle": "^6.4 || ^7.0 || ^8.0",
        "symfony/property-access": "^6.4 || ^7.0 || ^8.0",
        "symfony/serializer": "^6.4 || ^7.0 || ^8.0",
        "symfony/uid": "^6.4 || ^7.0 || ^8.0",
        "symfony/validator": "^6.4.11 || ^7.0 || ^8.0",
        "symfony/yaml": "^6.4 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "ApiPlatform\\Doctrine\\Odm\\": ""
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.3.x-dev",
            "dev-4.2": "4.2.x-dev",
            "dev-3.4": "3.4.x-dev",
            "dev-4.1": "4.1.x-dev"
        },
        "symfony": {
            "require": "^6.4 || ^7.0 || ^8.0"
        },
        "thanks": {
            "name": "api-platform/api-platform",
            "url": "https://github.com/api-platform/api-platform"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/soyuka/phpunit"
        }
    ]
}
