      {"type": "path", "url": "./src/RamseyUuid"},
      {"type": "path", "url": "./src/Serializer"},
      {"type": "path", "url": "./src/State"},
      {"type": "path", "url": "./src/Symfony"},
      {"type": "path", "url": "./src/Validator"},
      {"type": "path", "url": "./src/Laravel"},
      {"type": "path", "url": "./src/JsonApi"}
