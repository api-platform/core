{
  "name": "dunglas/api-bundle",
  "type": "symfony-bundle",
  "description": "JSON-LD / Hydra REST API for Symfony",
  "keywords": ["REST", "API", "JSON", "JSON-LD", "Hydra"],
  "homepage": "https://api-platform.com",
  "license": "MIT",
  "authors": [
    {
      "name": "KÃ©vin Dunglas",
      "email": "dunglas@gmail.com",
      "homepage": "https://dunglas.fr"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dunglas/DoctrineBundle"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/http-foundation": "~2.3|~3.0",
    "symfony/serializer": "~3.1@dev",
    "doctrine/inflector": "~1.0",
    "willdurand/negotiation": "~2.0"
  },
  "require-dev": {
    "friendsofsymfony/user-bundle": "~2.0@dev",
    "behat/behat": "~3.0",
    "behat/symfony2-extension": "~2.0",
    "behat/mink": "~1.5",
    "behat/mink-extension": "~2.0",
    "behat/mink-browserkit-driver": "~1.1",
    "behatch/contexts": "dev-master",
    "symfony/framework-bundle": "~3.1@dev",
    "symfony/finder": "~2.3",
    "symfony/security": "~2.7|~3.0",
    "symfony/property-info": "~3.1@dev",
    "symfony/dependency-injection": "~2.8@dev",
    "symfony/validator": "~2.5|~3.0",
    "phpdocumentor/reflection": "^1.0.7",
    "doctrine/orm": "~2.2,>=2.2.3",
    "doctrine/doctrine-bundle": "dev-property_info",
    "php-mock/php-mock-phpunit": "~1.1"
  },
  "suggest": {
    "friendsofsymfony/user-bundle": "To use the FOSUserBundle bridge."
  },
  "autoload": {
    "psr-4": { "ApiPlatform\\Core\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "ApiPlatform\\Core\\Tests\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
